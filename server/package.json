{
  "name": "web-scraping-assistant",
  "version": "1.0.0",
  "description": "A user-friendly web application for extracting textual content from URLs",
  "main": "server/index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup": "node setup.js",
    "test": "node test/test-api.js"
  },
  "keywords": ["web-scraping", "firecrawl", "react", "nodejs"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@mendable/firecrawl-js": "^1.29.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "axios": "^1.6.0"
  }
} 